	"author": {
		"name": "Filip Seman",
		"email": "filip.seman@protonmail.com"
	},
	"scripts": {
		"prebuild": "rm -rf ./lib || :",
		"build": "tsc",
		"postbuild": "chmod +x ./lib/cli.js",
		"test": "xv --loader=tsx ./src",
		"test:watch": "find ./src/*.ts | entr xv --loader=tsx ./src",
		"cli:local": "npm run build && npm link bysquare",
		"version": "git flow release start v$npm_package_version",
		"prepare": "",
		"prepublishOnly": "npm run build",
		"preversion": "git checkout develop",
		"postversion": ""
	},
	"dependencies": {
		"lodash.deburr": "~4.1.0",
		"lzma-native": "7.0.1",
		"rfc4648": "1.5.2"
	},
	"devDependencies": {
		"@types/lodash.deburr": "~4.1.0",
		"@types/lzma-native": "^4.0.1",
		"@types/node": "~18.11.0",
		"tsx": "~3.12.0",
		"typescript": "~4.9.0",
		"xv": "~2.1.0"
	},
	"bin": "lib/cli.js",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib",
		"!lib/*.test.*"
	],
	"engines": {
		"node": ">=16.0",
		"npm": ">=7.0"
	},
